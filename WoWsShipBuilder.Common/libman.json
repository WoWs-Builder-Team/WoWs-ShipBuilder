{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "chart.js@4.4.0",
      "destination": "wwwroot/lib/chartJs/",
      "files": [
        "dist/chart.umd.min.js"
      ]
    },
    {
      "library": "chartjs-plugin-autocolors@0.2.2",
      "destination": "wwwroot/lib/autocolor/",
      "files":[
        "dist/chartjs-plugin-autocolors.min.js"
      ]
    },
    {
      "library": "html2canvas@1.4.1",
      "destination": "wwwroot/lib/html2canvas/",
      "files":[
        "dist/html2canvas.min.js"
      ]
    },
    {
      "library": "https://cdn.jsdelivr.net/gh/linways/table-to-excel@v1.0.4/dist/tableToExcel.js",
      "destination": "wwwroot/lib/tableToExcel/",
      "provider": "filesystem",
      "files":[
        "dist/tableToExcel.js"
      ]
    }
  ]
}
