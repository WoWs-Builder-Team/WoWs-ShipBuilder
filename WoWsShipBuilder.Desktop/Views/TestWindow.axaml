<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:userControls="clr-namespace:WoWsShipBuilder.Desktop.UserControls"
        xmlns:customControls="clr-namespace:WoWsShipBuilder.Desktop.CustomControls"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="300"
        x:Class="WoWsShipBuilder.Desktop.Views.TestWindow"
        Title="TestWindow">
    <Grid RowDefinitions="30,auto,*">
        <userControls:AppHeader Grid.Row="0"/>
        <customControls:DepthChargeDamageDistributionChart Grid.Row="2" DcDmg="{Binding DcDmg}" SplashRadius="{Binding SplashRadius}" PointsOfDmg="{Binding PointsOfDmg}"/>
    </Grid>
</Window>
