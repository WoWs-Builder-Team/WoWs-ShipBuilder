<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:infrastructure="clr-namespace:WoWsShipBuilder.Desktop.Infrastructure"
        xmlns:userControls="clr-namespace:WoWsShipBuilder.Desktop.UserControls"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="WoWsShipBuilder.Desktop.Features.BlazorWebView.BlazorWindow"
        WindowStartupLocation="CenterScreen"
        Background="#ff282828"
        Title="BlazorWindow">
    <Grid RowDefinitions="30, *">
        <userControls:AppHeader ShowMaximizeButton="True" />
        <infrastructure:BlazorWebView Grid.Row="1" Name="WebView"
                                      HostPage="index.html"
                                      RootComponents="{DynamicResource rootComponents}"
                                      Services="{DynamicResource services}" />
    </Grid>
</Window>
